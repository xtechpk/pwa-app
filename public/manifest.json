{
  "name": "My PWA App",
  "short_name": "PWAApp",
  "start_url": "/",
  "display": "standalone",
  "display_override": "standalone" ,
  "description": "A Progressive Web App built with React and TypeScript",
  "lang": "en",
  "dir": "auto",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "orientation": "any",
  "icons": [
    {
      "src": "/icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/screen1.png",
      "sizes": "2880x1800",
      "type": "image/png",
      "description": "A screenshot of the home page"
    }
  ],
  "related_applications": [
    {
      "platform": "windows",
      "url": "https://localhost:3000/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "shortcuts": [
    {
      "name": "Dashboard",
      "url": "/dashboard",
      "description": "Go directly to the dashboard"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+myapp",
      "url": "/?q=%s"
    }
  ]
}
